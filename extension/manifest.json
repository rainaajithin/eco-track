{
  "manifest_version": 3,
  "name": "Carbon Footprint Tracker",
  "version": "1.1",
  "permissions": [
    "webRequest",
    "storage",
    "notifications",
    "activeTab"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Eco COâ‚‚ Tracker"},

      "web_accessible_resources": [
    {
      "resources": ["dashboard.html"],
      "matches": ["<all_urls>"]
    }
  ]
  
  }
